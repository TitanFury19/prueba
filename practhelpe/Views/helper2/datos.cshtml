@model practhelpe.Models.Class1
@{
    ViewBag.Title = "datos";
}

<!DOCTYPE html>

<h2>Datos</h2>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Registro de tareas</title>
</head>
<body>

    <div class="form-group">
        <div class="col-md-10">
            <label>La Cedula es: @Model.cedula <br /></label>
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-10">
            <label>El Nombre es: @Model.nombre <br /></label>
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-10">
            <label>El Apellido es: @Model.apellido <br /></label>
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-10">
            <label>La edad es: @Model.edad <br /></label>
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-10">
            <label>El telefono es: @Model.telefono <br /></label>
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-10">
            <label>El correo es: @Model.correo <br /></label>
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-10">
            <label>El genero es: @Model.genero <br /></label>
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-10">
            <label>Su estado civil es: @Model.estadoc <br /></label>
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-10">
            <label>Su Hobby es: @Model.hobbys <br /></label>
        </div>
    </div>

</body>
</html>
